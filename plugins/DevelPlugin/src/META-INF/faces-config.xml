<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

  <managed-bean>
    <managed-bean-name>DevelBean</managed-bean-name>
    <managed-bean-class>sonia.blog.devel.DevelBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>

  <navigation-rule>
    <from-view-id>/personal/*</from-view-id>
    <navigation-case>
      <from-outcome>devel</from-outcome>
      <to-view-id>/view/devel/devel.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
    
</faces-config>
